<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <servlet>
        <servlet-name>testServlet</servlet-name>
        <servlet-class>com.yaser.core.servlet.testServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>defaultServlet</servlet-name>
        <servlet-class>com.yaser.core.servlet.impl.DefaultServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>testServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>defaultServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
</web-app>